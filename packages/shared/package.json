{
  "name": "@rttnd/llm-shared",
  "type": "module",
  "version": "0.6.3",
  "description": "Shared types and data for LLM Registry",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.mjs"
    },
    "./registry": {
      "types": "./dist/registry.d.ts",
      "default": "./dist/registry.mjs"
    },
    "./search": {
      "types": "./dist/search.d.ts",
      "default": "./dist/search.mjs"
    },
    "./custom": {
      "types": "./dist/custom.d.ts",
      "default": "./dist/custom.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "check": "tsgo --noEmit",
    "release": "bun publish"
  },
  "devDependencies": {
    "tsdown": "catalog:dev"
  }
}
