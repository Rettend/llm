{
  "name": "@rttnd/llm-shared",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "Shared types and data for LLM Registry",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.mjs"
    },
    "./capabilities": {
      "types": "./dist/capabilities.d.ts",
      "default": "./dist/capabilities.mjs"
    },
    "./search": {
      "types": "./dist/search.d.ts",
      "default": "./dist/search.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "check": "tsgo --noEmit"
  },
  "devDependencies": {
    "tsdown": "^0.16.1"
  }
}
