{
  "name": "@rttnd/llm-server",
  "type": "module",
  "version": "1.4.0",
  "private": true,
  "description": "LLM Registry API Server",
  "exports": {
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "deploy": "wrangler deploy",
    "cf:dev": "wrangler dev",
    "cf:tail": "wrangler tail",
    "lint": "eslint . --fix",
    "check": "tsgo --noEmit",
    "test": "bun test",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@elysiajs/cors": "catalog:prod",
    "@elysiajs/openapi": "catalog:prod",
    "@rttnd/llm-shared": "^1.4.0",
    "elysia": "catalog:prod"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "catalog:dev",
    "@types/bun": "catalog:dev",
    "@typescript/native-preview": "catalog:dev",
    "wrangler": "catalog:dev"
  }
}
